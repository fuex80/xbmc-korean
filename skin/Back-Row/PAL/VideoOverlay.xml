<window>
<id>2004</id>
<defaultcontrol>2</defaultcontrol>
<controls>

<control type="group">
<posx>0</posx>
<posy>149</posy>

<animation effect="slide" time="600" start="-328,0" end="0,0" acceleration="-1">WindowOpen</animation>
<animation effect="slide" time="600" start="0,0" end="-328,0" acceleration="-1">WindowClose</animation>

<control type="image">
<description>Background image</description>
<posx>0</posx>
<posy>0</posy>
<width>328</width>
<height>278</height>
<texture>List background2.png</texture>
<aspectratio>stretch</aspectratio>
<visible>!Window.IsActive(3) + !Window.IsActive(4)</visible>
<visible>!Window.IsActive(7) + !Window.IsActive(56) + !Window.IsActive(2700)</visible>
<visible>!Window.IsActive(AppearanceSettings) + !Window.IsActive(MyVideosSettings)</visible>
<visible>!Window.IsActive(MyMusicSettings) + !Window.IsActive(SystemSettings)</visible>
<visible>Player.HasMedia + !Skin.HasSetting(video-preview)</visible>
<animation effect="fade" time="300">Visible</animation>
</control>


<control type="image">
<description>Background image</description>
<posx>0</posx>
<posy>0</posy>
<width>328</width>
