<includes>
	<include name="FullScreenMovieResolutionflagsVars">
		<width>59</width>
		<height>40</height>
		<visible>IsEmpty(VideoPlayer.VideoResolution)</visible>
	</include>
	<include name="FullScreenMovieVideoCodecflagsVars">
		<width>59</width>
		<height>40</height>
		<visible>IsEmpty(VideoPlayer.VideoCodec)</visible>
	</include>
	<include name="FullScreenMovieAudioCodecflagsVars">
		<width>59</width>
		<height>40</height>
		<visible>IsEmpty(VideoPlayer.AudioCodec)</visible>
	</include>
	<include name="FullScreenMovieSubtitlesflagsVars">
		<width>59</width>
		<height>40</height>
		<visible>IsEmpty(VideoPlayer.SubtitleLanguage)</visible>
	</include>
	<include name="FullScreenMovieStudioflagsVarsSmall">
		<width>59</width>
		<height>40</height>
	</include>
	<include name="FullScreenMovieMediaflagsVars">
		<width>59</width>
		<height>40</height>
	</include>
	<include name="FullScreenMovieMediaflags">
		<control type="grouplist">
			<posx>255</posx>
			<posy>145</posy>
			<width>577</width>
			<height>40</height>
			<itemgap>13</itemgap>
			<orientation>horizontal</orientation>
		<!-- Resolution (Native) -->
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/$INFO[VideoPlayer.VideoResolution,,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoResolution)</visible>
			</control>
		<!-- Resolution (Studio Tag) -->
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/sd.png</texture>
				<visible>substring(VideoPlayer.studio,SDp) | substring(VideoPlayer.studio,SDi)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/480p.png</texture>
				<visible>substring(VideoPlayer.studio,480p)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/576p.png</texture>
				<visible>substring(VideoPlayer.studio,576p)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/720p.png</texture>
				<visible>substring(VideoPlayer.studio,720p)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/1080p.png</texture>
				<visible>substring(VideoPlayer.studio,1080p)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/480i.png</texture>
				<visible>substring(VideoPlayer.studio,480i)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/576i.png</texture>
				<visible>substring(VideoPlayer.studio,576i)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/720i.png</texture>
				<visible>substring(VideoPlayer.studio,720i)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieResolutionflagsVars</include>
				<texture>special://skin/images/flags/movies/resolution/1080i.png</texture>
				<visible>substring(VideoPlayer.studio,1080i)</visible>
			</control>
		<!-- AspectRatio (Native) -->
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/aspectratio/$INFO[VideoPlayer.VideoAspect,,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoAspect)</visible>
			</control>
		<!-- Video Codec (Native) -->
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/$INFO[VideoPlayer.VideoCodec,,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.VideoCodec)</visible>
			</control>
		<!-- Video Codec  (Studio Tag) -->
			<control type="image">
				<include>FullScreenMovieVideoCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/divx.png</texture>
				<visible>substring(VideoPlayer.studio,DX50) | substring(VideoPlayer.studio,divx) | substring(VideoPlayer.studio,DIV3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieVideoCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/xvid.png</texture>
				<visible>substring(VideoPlayer.studio,xvid)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieVideoCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/h.264.png</texture>
				<visible>substring(VideoPlayer.studio,V_MPEG4)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieVideoCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/quicktime.png</texture>
				<visible>substring(VideoPlayer.studio,SVQ3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieVideoCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/wmv.png</texture>
				<visible>substring(VideoPlayer.studio,WMV) | substring(VideoPlayer.studio,WMV2)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieVideoCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/videocodec/wmvhd.png</texture>
				<visible>substring(VideoPlayer.studio,WVC1)</visible>
			</control>
		<!-- Audio Codec (Native) -->
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/aac.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,aac)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mono.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,ac3) | substring(VideoPlayer.AudioCodec,ac-3)] + substring(VideoPlayer.AudioChannels,1)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dolby20.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,ac3) | substring(VideoPlayer.AudioCodec,ac-3)] + substring(VideoPlayer.AudioChannels,2)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd40.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,ac3) | substring(VideoPlayer.AudioCodec,ac-3)] + substring(VideoPlayer.AudioChannels,4)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd41.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,ac3) | substring(VideoPlayer.AudioCodec,ac-3)] + substring(VideoPlayer.AudioChannels,5)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd51.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,ac3) | substring(VideoPlayer.AudioCodec,ac-3)] + substring(VideoPlayer.AudioChannels,6)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd71.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,ac3) | substring(VideoPlayer.AudioCodec,ac-3)] + substring(VideoPlayer.AudioChannels,8)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dts.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,dca) | substring(VideoPlayer.AudioCodec,dts)] + substring(VideoPlayer.AudioChannels,2)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dts51.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,dca) | substring(VideoPlayer.AudioCodec,dts)] + substring(VideoPlayer.AudioChannels,6)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dts71.png</texture>
				<visible>[substring(VideoPlayer.AudioCodec,dca) | substring(VideoPlayer.AudioCodec,dts)] + substring(VideoPlayer.AudioChannels,8)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mpeg audio.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,mp1)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mp2.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,mp2) | substring(VideoPlayer.AudioCodec,mpeg audio)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mp3.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,mp3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/ogg.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,vorbis)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/wav.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,pcm)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/wma.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,wmav2) | substring(VideoPlayer.AudioCodec,wma2)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/wmahd.png</texture>
				<visible>substring(VideoPlayer.AudioCodec,wmapro) | substring(VideoPlayer.AudioCodec,wma3)</visible>
			</control>
		<!-- Audio Codec (Studio Tag) -->
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dts.png</texture>
				<visible>substring(VideoPlayer.studio,dts) + ![substring(VideoPlayer.studio,6ch) | substring(VideoPlayer.studio,8ch)]</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd40.png</texture>
				<visible>substring(VideoPlayer.studio,4ch) + substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd41.png</texture>
				<visible>substring(VideoPlayer.studio,5ch) + substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd51.png</texture>
				<visible>substring(VideoPlayer.studio,6ch) + substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dd71.png</texture>
				<visible>substring(VideoPlayer.studio,8ch) + substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dts51.png</texture>
				<visible>substring(VideoPlayer.studio,6ch) + substring(VideoPlayer.studio,dts)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dts71.png</texture>
				<visible>substring(VideoPlayer.studio,8ch) + substring(VideoPlayer.studio,dts)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dolby20.png</texture>
				<visible>substring(VideoPlayer.studio,2ch) + substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dolbypro.png</texture>
				<visible>substring(VideoPlayer.studio,dolbypro)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mono.png</texture>
				<visible>substring(VideoPlayer.studio,1ch) + substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dolbytruehd.png</texture>
				<visible>substring(VideoPlayer.studio,truehd) | substring(VideoPlayer.studio,true hd)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/dtsma.png</texture>
				<visible>substring(VideoPlayer.studio,dtsma) | substring(VideoPlayer.studio,master audio) + !substring(VideoPlayer.studio,ac-3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mp2.png</texture>
				<visible>substring(VideoPlayer.studio,MPEG Audio) + ![substring(VideoPlayer.studio,DX50) | substring(VideoPlayer.studio,divx) | substring(VideoPlayer.studio,DIV3) | substring(VideoPlayer.studio,xvid)] + [substring(VideoPlayer.studio,SD) | substring(VideoPlayer.studio,480) | substring(VideoPlayer.studio,540) | substring(VideoPlayer.studio,576)]</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/mp3.png</texture>
				<visible>substring(VideoPlayer.studio,MPEG Audio) + [substring(VideoPlayer.studio,DX50) | substring(VideoPlayer.studio,divx) | substring(VideoPlayer.studio,DIV3) | substring(VideoPlayer.studio,xvid)]</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/flac.png</texture>
				<visible>substring(VideoPlayer.studio,flac)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/ogg.png</texture>
				<visible>substring(VideoPlayer.studio,vorbis)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/aac.png</texture>
				<visible>substring(VideoPlayer.studio,aac)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/wav.png</texture>
				<visible>substring(VideoPlayer.studio,pcm)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/wma.png</texture>
				<visible>substring(VideoPlayer.studio,WMA) + !substring(VideoPlayer.studio,WMA3)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieAudioCodecflagsVars</include>
				<texture>special://skin/images/flags/movies/audiocodec/wmahd.png</texture>
				<visible>substring(VideoPlayer.studio,WMA3)</visible>
			</control>
		<!-- Subtitles (Studio Tag) -->
			<control type="image">
				<include>FullScreenMovieSubtitlesflagsVars</include>
				<texture>special://skin/images/flags/movies/subtitles/cc.png</texture>
				<visible>substring(VideoPlayer.studio,sub)</visible>
			</control>
		<!-- Rating -->
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/rating/rating-g.png</texture>
				<visible>substring(VideoPlayer.mpaa,Rated G)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/rating/rating-pg.png</texture>
				<visible>substring(VideoPlayer.mpaa,Rated PG) + !substring(VideoPlayer.mpaa,Rated PG-13)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/rating/rating-pg13.png</texture>
				<visible>substring(VideoPlayer.mpaa,Rated PG-13)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/rating/rating-r.png</texture>
				<visible>substring(VideoPlayer.mpaa,Rated R)</visible>
			</control>
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/rating/rating-nc17.png</texture>
				<visible>substring(VideoPlayer.mpaa,Rated NC-17)</visible>
			</control>
		<!-- Studio -->
			<control type="image">
				<include>FullScreenMovieMediaflagsVars</include>
				<texture>special://skin/images/flags/movies/studio/$INFO[VideoPlayer.Studio,,.png]</texture>
				<visible>!IsEmpty(VideoPlayer.Studio)</visible>
			</control>
		</control>
	</include>
</includes>
