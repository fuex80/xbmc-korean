<window id="2901">
<defaultcontrol always="true">218</defaultcontrol>
  
<controls>

  <control type="group">
    <posx>20</posx>
    <posy>485</posy>
    <include>SlideFromBottom</include>
    <include>OSDBack</include>
    <visible>!Window.IsActive(123) + !Window.IsActive(124) + !Window.IsActive(125)</visible>
    <control type="label">
      <description>Label</description>
      <posx>10</posx>
      <posy>10</posy>
      <width>520</width>
      <height>30</height>
      <align>left</align>
      <aligny>middle</aligny>
      <font>20BdFont</font>
      <label>$INFO[VideoPlayer.Label]</label>
      <textcolor>FocusedTextColor</textcolor>
      <scroll>true</scroll>
      <visible>IsEmpty(VideoPlayer.Title)</visible>
    </control>
    <control type="label">
      <description>Label</description>
      <posx>10</posx>
      <posy>10</posy>
      <width>520</width>
      <height>30</height>
      <align>left</align>
      <aligny>middle</aligny>
      <font>20BdFont</font>
      <label>$INFO[VideoPlayer.Title]</label>
      <textcolor>FocusedTextColor</textcolor>
      <scroll>true</scroll>
    </control>

    <control type="group" id="3000">
      <control type="button" id="215">
        <description>Prev Track button</description>
        <posx>10</posx>
        <posy>40</posy>
        <width>101</width>
        <font>18BdFont</font>
        <label>$LOCALIZE[31034]</label>
        <onclick>XBMC.PlayerControl(Previous)</onclick>
        <onleft>220</onleft>
        <onright>216</onright>
        <onup>3001</onup>
        <ondown>3001</ondown>
      </control>
      <control type="button" id="216">
        <description>Rewind button</description>
        <posx>121</posx>
        <posy>40</posy>
        <width>101</width>
        <font>18BdFont</font>
        <label>$LOCALIZE[31035]</label>
        <onclick>XBMC.PlayerControl(Rewind)</onclick>
        <onleft>215</onleft>
        <onright>217</onright>
        <onup>3001</onup>
        <ondown>3001</ondown>
      </control>
      <control type="button" id="217">
        <description>Stop button</description>
        <posx>233</posx>
        <posy>40</posy>
        <width>101</width>
        <font>18BdFont</font>
        <label>$LOCALIZE[31036]</label>
        <onclick>XBMC.PlayerControl(Stop)</onclick>
        <onleft>216</onleft>
        <onright>218</onright>
        <onup>3001</onup>
        <ondown>3001</ondown>
      </control>
      <control type="togglebutton" id="218">
        <description>Play/Pause button</description>
        <posx>345</posx>
        <posy>40</posy>
        <width>101</width>
        <font>18BdFont</font>
        <label>$LOCALIZE[31037]</label>
        <altlabel>$LOCALIZE[31040]</altlabel>
        <usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
        <onclick>XBMC.PlayerControl(Play)</onclick>
        <onleft>217</onleft>
        <onright>219</onright>
        <onup>3001</onup>
        <ondown>3001</ondown>
      </control>
      <control type="button" id="219">
        <description>Fast Forward button</description>
        <posx>457</posx>
        <posy>40</posy>
        <width>101</width>
        <font>18BdFont</font>
        <label>$LOCALIZE[31038]</label>
        <onclick>XBMC.PlayerControl(Forward)</onclick>
        <onleft>218</onleft>
        <onright>220</onright>
        <onup>3001</onup>
        <ondown>3001</ondown>
      </control>
      <control type="button" id="220">
        <description>Skip Fwd</description>
        <posx>569</posx>
        <posy>40</posy>
        <width>101</width>
        <font>18BdFont</font>
        <label>$LOCALIZE[31039]</label>
        <onclick>XBMC.PlayerControl(Next)</onclick>
        <onleft>219</onleft>
        <onright>215</onright>
        <onup>3001</onup>
        <ondown>3001</ondown>
      </control>
    </control>
    
    <control type="group" id="3001">
      <posx>540</posx>
      <control type="button" id="602">
        <description>DVD button</description>
        <posy>10</posy>
        <width>25</width>
        <height>25</height>
        <font>-</font>
        <label>-</label>
        <texturefocus>OSDDVDFo.png</texturefocus>
        <texturenofocus>OSDDVDNF.png</texturenofocus>
        <onclick>XBMC.PlayerControl(ShowVideoMenu)</onclick>
        <onleft>605</onleft>
        <onright>603</onright>
        <ondown>3000</ondown>
        <onup>3000</onup>
        <visible>VideoPlayer.HasMenu</visible>
      </control>
      <control type="button" id="603">
        <description>Bookmarks button</description>
        <posx>35</posx>
        <posy>10</posy>
        <width>25</width>
        <height>25</height>
        <font>-</font>
        <label>-</label>
        <texturefocus>OSDBookmarksFo.png</texturefocus>
        <texturenofocus>OSDBookmarksNF.png</texturenofocus>
        <onclick>XBMC.ActivateWindow(125)</onclick>
        <onleft>602</onleft>
        <onright>604</onright>
        <ondown>3000</ondown>
        <onup>3000</onup>
      </control>
      <control type="button" id="604">
        <description>Video Settings button</description>
        <posx>70</posx>
        <posy>10</posy>
        <width>25</width>
        <height>25</height>
        <font>-</font>
        <label>-</label>
        <texturefocus>OSDVideoFo.png</texturefocus>
        <texturenofocus>OSDVideoNF.png</texturenofocus>
        <onclick>XBMC.ActivateWindow(123)</onclick>
        <onleft>603</onleft>
        <onright>605</onright>
        <ondown>218</ondown>
        <onup>218</onup>
      </control>
      <control type="button" id="605">
        <description>Audio Settings button</description>
        <posx>105</posx>
        <posy>10</posy>
        <width>25</width>
        <height>25</height>
        <font>-</font>
        <label>-</label>
        <texturefocus>OSDAudioFo.png</texturefocus>
        <texturenofocus>OSDAudioNF.png</texturenofocus>
        <onclick>XBMC.ActivateWindow(124)</onclick>
        <onleft>604</onleft>
        <onright>602</onright>
        <ondown>3000</ondown>
        <onup>3000</onup>
      </control>
    </control>
  </control>

</controls>

</window>
