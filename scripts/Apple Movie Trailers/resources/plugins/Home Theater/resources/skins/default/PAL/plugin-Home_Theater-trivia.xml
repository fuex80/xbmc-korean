<window>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="0">WindowClose</animation>
    <allowoverlays>false</allowoverlays>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
		<control type="image" id="100">
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<texture>-</texture>
            <aspectratio>keep</aspectratio>
		</control>
	</controls>
</window>