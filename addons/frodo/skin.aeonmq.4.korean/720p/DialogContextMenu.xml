<!-- Aeon MQ 4 -->

<window id="106">
	<defaultcontrol>1000</defaultcontrol>
		<zorder>3</zorder>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="144,214" easing="out" tween="back" time="300" />
		<effect type="fade" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="144,214" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	  <animation effect="slide" time="0" tween="cubic" easing="out" end="400,0" condition="Window.IsVisible(146)">Conditional</animation>
	<controls>
	<control type="image" id="999">
    <description>background image</description>
    <posx>0</posx>
    <posy>0</posy>
    <width>456</width>
    <height>720</height>
    <texture>-</texture>
  </control>
	<control type="image" id="997">
    <description>top image</description>
    <posx>-2</posx>
    <posy>23</posy>
    <width>463</width>
    <height>27</height>
    <texture>dialogs_context_top.png</texture>
  </control>
	  <control type="image" id="998">
    <description>bottom image</description>
    <posx>-2</posx>
    <posy>0</posy>
    <width>463</width>
    <height>27</height>
    <texture>dialogs_context_bottom.png</texture>
  </control>
  <control type="grouplist" id="996">
    <description>grouplist for context buttons</description>
    <posx>0</posx>
    <posy>50</posy>
    <width>460</width>
    <height max="670">auto</height>
    <itemgap>0</itemgap>
  </control>
  <control type="button" id="1000">
    <description>button template</description>
    <posx>0</posx>
    <posy>0</posy>
    <height>39</height>
    <width>460</width>
    <onleft>Close</onleft>
    <onright>Close</onright>
    <textcolor>FFe5e5e5</textcolor>
    <focusedcolor>FF101010</focusedcolor>
    <texturenofocus>dialogs_context_nofocus.png</texturenofocus>
    <texturefocus>dialogs_context_focus.png</texturefocus>
    <pulseonselect>true</pulseonselect>
    <align>center</align>
    <aligny>top</aligny>
    <font>Font_SettingsOptions</font>
    <textoffsety>8</textoffsety>
    <colordiffuse>$VAR[value_texturecolors]</colordiffuse>
  </control>
	</controls>
</window>