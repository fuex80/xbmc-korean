<!-- Aeon MQ 4 -->

<window id="120">
	<defaultcontrol>101</defaultcontrol>
	<controls>
  
  <control type="button" id="4097">
    <include>InvisibleButton</include>
    <onfocus>SetFocus(20)</onfocus>
    <onfocus>Skin.Reset(vis_fanartbg)</onfocus>
    <onfocus>Skin.SetBool(vis_visbg)</onfocus>
  </control>
  <control type="button" id="4098">
    <include>InvisibleButton</include>
    <onfocus>SetFocus(101)</onfocus>
    <onfocus>Skin.Reset(vis_visbg)</onfocus>
    <onfocus>Skin.SetBool(vis_fanartvisbg)</onfocus>
  </control>
  <control type="button" id="4099">
    <include>InvisibleButton</include>
    <onfocus>SetFocus(101)</onfocus>
    <onfocus>Skin.Reset(vis_fanartvisbg)</onfocus>
    <onfocus>Skin.SetBool(vis_fanartbg)</onfocus>
  </control>
  <control type="button" id="4111">
    <include>InvisibleButton</include>
    <onfocus>SetFocus(1)</onfocus>
    <onfocus>Skin.Reset(novisfurniture)</onfocus>
    <onfocus>Skin.SetBool(furniture_cdart)</onfocus>
  </control>
  <control type="button" id="4112">
    <include>InvisibleButton</include>
    <onfocus>SetFocus(1)</onfocus>
    <onfocus>Skin.SetBool(novisfurniture)</onfocus>
    <onfocus>Skin.Reset(furniture_cdart)</onfocus>
  </control>
  <control type="button" id="4113">
    <include>InvisibleButton</include>
    <onfocus>SetFocus(1)</onfocus>
    <onfocus>Skin.Reset(novisfurniture)</onfocus>
    <onfocus>Skin.Reset(furniture_cdart)</onfocus>
  </control>

  <control type="group">
    <posy>720</posy>
  <animation type="WindowOpen" condition="true">
    <effect type="slide" time="400" start="0,0" end="0,-200" easing="out" tween="cubic"/>
  </animation>
  <animation type="WindowClose" condition="true">
    <effect type="slide" time="400" start="0,-200" end="0,0" easing="in" tween="cubic"/>
  </animation>
  
  <!-- Seek Widget -->
  <control type="group">
    <posx>-500</posx>
    <posy>38</posy>
    <width>499</width>
    <height>28</height>
    <animation effect="slide" end="200,0" time="200" condition="Player.Forwarding2x">Conditional</animation>
    <animation effect="slide" end="275,0" time="200" condition="Player.Forwarding4x">Conditional</animation>
    <animation effect="slide" end="350,0" time="200" condition="Player.Forwarding8x">Conditional</animation>
    <animation effect="slide" end="425,0" time="200" condition="Player.Forwarding16x">Conditional</animation>
    <animation effect="slide" end="499,0" time="200" condition="Player.Forwarding32x">Conditional</animation>
  <control type="image">
    <posx>0</posx>
    <posy>0</posy>
    <width>499</width>
    <height>28</height>
    <texture flipy="true">osd_button_reverse.png</texture>
  </control>
  <control type="label">
    <include>Objects_OSDSeekForward</include>
    <label>$VAR[value_seekforward]</label>
  </control>
  </control>
  <control type="group">
    <posx>1280</posx>
    <posy>38</posy>
    <width>499</width>
    <height>28</height>
    <animation effect="slide" end="-200,0" time="200" condition="Player.Rewinding2x">Conditional</animation>
    <animation effect="slide" end="-275,0" time="200" condition="Player.Rewinding4x">Conditional</animation>
    <animation effect="slide" end="-350,0" time="200" condition="Player.Rewinding8x">Conditional</animation>
    <animation effect="slide" end="-425,0" time="200" condition="Player.Rewinding16x">Conditional</animation>
    <animation effect="slide" end="-499,0" time="200" condition="Player.Rewinding32x">Conditional</animation>
  <control type="image">
    <posx>0</posx>
    <posy>0</posy>
    <width>499</width>
    <height>28</height>
    <texture flipy="true" flipx="true">osd_button_reverse.png</texture>
  </control>
  <control type="label">
    <include>Objects_OSDSeekReverse</include>
    <label>$VAR[value_seekreverse]</label>
  </control>
  </control>
  <control type="image">
    <posx>0</posx>
    <posy>-80</posy>
    <height>270</height>
    <width>1280</width>
    <texture background="true">osd_black_bg.png</texture>
  </control>
  
  <!-- Menu Content -->
  <control type="image">
    <posx>0</posx>
    <posy>60</posy>
    <height>140</height>
    <width>1280</width>
    <texture background="true">panel_listlow.png</texture>
    <colordiffuse>$VAR[value_texturecolors]</colordiffuse>
  </control>
    <include condition="!Skin.HasSetting(audioosdfixedlist)">AudioOSDWrapList</include>
    <include condition="Skin.HasSetting(audioosdfixedlist)">AudioOSDFixedList</include>
 
  <control type="progress">
    <posx>30</posx>
    <posy>73</posy>
    <width>1225</width>
    <height>25</height>
    <info>Player.Progress</info>
    <animation effect="fade" end="90" condition="true">Conditional</animation>
  </control>
  <control type="slider" id="888">
    <description>Slider</description>
    <posx>39</posx>
    <posy>73</posy>
    <width>1202</width>
    <height>21</height>
    <onup>Close</onup>
    <ondown>101</ondown>
    <action>seek</action>
    <texturesliderbar>osd_sliderback_large.png</texturesliderbar>
    <textureslidernib>-</textureslidernib>
    <textureslidernibfocus>osd_progress_mid.png</textureslidernibfocus>
    <animation effect="fade" time="200">VisibleChange</animation>
    <animation effect="fade" end="50" condition="true">Conditional</animation>
  </control>
  <control type="label">
    <posx>340</posx>
    <posy>75</posy>
    <width>600</width>
    <height>30</height>
    <align>center</align>
    <font>Font_Time</font>
    <textcolor>FFececec</textcolor>
    <label>$VAR[value_osd_timeduration]</label>
  </control>
  <control type="label"><!-- icons label -->
    <posx>320</posx>
    <posy>172</posy>
    <width>640</width>
    <height>25</height>
    <align>center</align>
    <font>Font_MediaWindowMenu</font>
    <label>$VAR[value_osdmusic_title]</label>
    <textcolor>FFececec</textcolor>
  </control>
  <control type="label"><!-- hora atual -->
    <posx>30</posx>
    <posy>130</posy>
    <width>300</width>
    <height>25</height>
    <font>Font_OSDOptions</font>
    <textcolor>70FFFFFF</textcolor>
    <label>31310</label>
  </control>
  <control type="label">
    <posx>30</posx>
    <posy>160</posy>
    <width>300</width>
    <height>25</height>
    <font>Font_ProgressLabel</font>
    <textcolor>FFececec</textcolor>
    <label>$INFO[System.Time]</label>
  </control>
  <control type="label"><!-- término -->
    <posx>1250</posx>
    <posy>130</posy>
    <width>300</width>
    <height>25</height>
    <align>right</align>
    <font>Font_OSDOptions</font>
    <textcolor>70FFFFFF</textcolor>
    <label>31311</label>
  </control>
  <control type="label">
    <posx>1250</posx>
    <posy>160</posy>
    <width>300</width>
    <height>25</height>
    <align>right</align>
    <font>Font_ProgressLabel</font>
    <textcolor>FFececec</textcolor>
    <label>$INFO[Player.FinishTime]</label>
  </control>
  <control type="image">
    <posx>0</posx>
    <posy>115</posy>
    <height>90</height>
    <width>1280</width>
    <texture>multiplex_seasongradient.png</texture>
  </control>
  <control type="image">
    <posx>0</posx>
    <posy>65</posy>
    <height>130</height>
    <width>1280</width>
    <colordiffuse>99FFFFFF</colordiffuse>
    <texture>panel_osd_glass.png</texture>
  </control>
  </control>
    <include>BGBlack</include>
  </controls>
</window>