<!-- Aeon MQ 4 -->

<window id="113">
	<controls>
  <control type="group">
  	<include>Animation_DialogPopup</include>
    <posx>550</posx>
    <posy>-15</posy>
  <animation type="WindowOpen" reversible="false">
    <effect type="fade" start="0" end="100" time="400" delay="350" />
    <effect type="slide" start="0,-200" end="0,0" time="400" tween="cubic" easing="out" delay="1500" />
  </animation>
  <animation type="WindowClose" reversible="false">
    <effect type="fade" start="100" end="0" time="200" delay="350" />
    <effect type="slide" start="0,0" end="0,-200" time="200" tween="cubic" easing="in" delay="350" />
  </animation>
  <control type="image">
    <posx>35</posx>
    <posy>0</posy>
    <width>90</width>
    <height>90</height>
    <aspectratio>keep</aspectratio>
    <texture>dialogs_volume_icon_mute.png</texture>
    <animation effect="fade" end="60" condition="true">Conditional</animation>
  </control>
  </control>
  </controls>
  </window>