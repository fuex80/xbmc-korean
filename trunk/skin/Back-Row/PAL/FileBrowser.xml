<window>
<id>126</id>
<defaultcontrol>450</defaultcontrol>
<allowoverlay>no</allowoverlay> 
<coordinates>
<system>1</system>
<posx>0</posx>
<posy>0</posy>
</coordinates>
<controls>
<controlgroup>
<animation effect="slide" time="600" start="650,0" end="0,0" acceleration="-1" condition="!Skin.HasSetting(animated)">WindowOpen</animation>
<animation effect="slide" time="600" start="0,0" end="730,0" acceleration="-1" condition="!Skin.HasSetting(animated)">WindowClose</animation>

<control type="image">
		<description>backgorund</description>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<texture>blackgloss.png</texture>
		<aspectratio>stretch</aspectratio>
		</control>

<control>
<description>Profile Background</description>
<type>image</type>
<id>1</id>
<posx>-10</posx>
<posy>55</posy>
<width>740</width>
<height>465</height>
<texture>File panel.png</texture>
</control>

<control type="label" id="411">
<description>heading label</description>
<posx>25</posx>
<posy>85</posy>
<width>350</width>
<align>left</align>
<font>Heading-M</font>
<label>1023</label>
<textcolor>red</textcolor>
</control>

<control type="label" id="412">
<description>path label control</description>
<posx>25</posx>
<posy>110</posy>
<width>350</width>
<align>left</align>
<font>font13</font>
<haspath>true</haspath>
</control>


<control>
<description>Listcontrol</description>
<id>450</id>
<type>list</type>
<posx>30</posx>
<posy>135</posy>
<width>320</width>
<height>315</height>
<onleft>413</onleft>
<onright>415</onright>
<onup>450</onup>
<ondown>450</ondown>
<pagecontrol>90</pagecontrol>

<itemlayout height="35">

<control type="image">
<posx>2</posx>
<posy>2.5</posy>
<width>20</width>
<height>30</height>
<aspectratio>keep</aspectratio>
<info>ListItem.Thumb</info>
</control>

<control type="label">
<posx>25</posx>
<posy>8</posy>
<width>270</width>
<height>25</height>
<font>font18</font>
<textcolor>white</textcolor>
<include>selectedcolor</include>
<info>ListItem.Label</info>
</control>
</itemlayout>

<focusedlayout height="35">

<control type="image">
<posx>2</posx>
<posy>2.5</posy>
<width>20</width>
<height>30</height>
<aspectratio>keep</aspectratio>
<info>ListItem.Thumb</info>
</control>

<control type="image">
<posx>0</posx>
<posy>0</posy>
<width>300</width>
<height>35</height>
<texture>input-focus.png</texture>
<visible>Control.HasFocus(450)</visible>
</control>

<control type="label">
<posx>25</posx>
<posy>8</posy>
<width>270</width>
<height>25</height>
<font>font18</font>
<textcolor>white</textcolor>
<include>selectedcolor</include>
<scroll>true</scroll>
<scrolltime>100</scrolltime>
<info>ListItem.Label</info>
</control>
</focusedlayout>
</control>

<control type="image">
<description>list item icon</description>
<posx>350</posx>
<posy>90</posy>
<width>340</width>
<height>390</height>
<info>ListItem.Icon</info>
<aspectratio>keep</aspectratio>
<visible>Control.IsVisible(450)</visible>
</control>



<control>
<description>OK button</description>
<type>button</type>
<id>413</id>
<posx>30</posx>
<posy>460</posy>
<width>100</width>
<height>35</height>
<align>center</align>
<label>186</label>
<font>font18</font>
<texturefocus>button-focus.png</texturefocus>
<texturenofocus>button-nofocus.png</texturenofocus>
<onleft>415</onleft>
<onright>414</onright>
<onup>450</onup>
<ondown>450</ondown>
<animation effect="fade" time="150">WindowOpen</animation>
<animation effect="fade" time="50">WindowClose</animation>
</control>

<control>
<description>Cancel button</description>
<type>button</type>
<id>414</id>
<posx>135</posx>
<posy>460</posy>
<width>100</width>
<height>35</height>
<align>center</align>
<label>222</label>
<font>font18</font>
<texturefocus>button-focus.png</texturefocus>
<texturenofocus>button-nofocus.png</texturenofocus>
<onleft>413</onleft>
<onright>415</onright>
<onup>450</onup>
<ondown>450</ondown>
<animation effect="fade" time="150">WindowOpen</animation>
<animation effect="fade" time="50">WindowClose</animation>
</control>

<control>
<description>Create folder button</description>
<type>button</type>
<id>415</id>
<posx>240</posx>
<posy>460</posy>
<width>100</width>
<height>35</height>
<align>center</align>
<label>119</label>
<font>font18</font>
<texturefocus>button-focus.png</texturefocus>
<texturenofocus>button-nofocus.png</texturenofocus>
<onleft>414</onleft>
<onright>413</onright>
<onup>450</onup>
<ondown>450</ondown>
<animation effect="fade" time="150">WindowOpen</animation>
<animation effect="fade" time="50">WindowClose</animation>
</control>






</controlgroup>
</controls>
</window>